ActionBar_Handler:
    type: world
    debug: false
    events:

        # REMOVED THE REPEATED EVALUATIONS OF CMI VANISHED
        # BY USING DEFINE - Nopal

        on player changes air level:
            - stop if:<player.oxygen.is_less_than[15]>
            - stop if:<player.has_flag[submerged]>
            - flag <player> submerged expire:1s
            - if <player.has_flag[accept_resourcepack]>:

                # IF PLAYER VANISH
                - if <placeholder[cmi_user_vanished_symbol].equals[].not>:
                    - actionbar "<script[ActionBar_Data].parsed_key[java.survival.up.vanish]>" targets:<player>

                # IF PLAYER NOT VANISH
                - else:
                    - actionbar "<script[ActionBar_Data].parsed_key[java.survival.up.unvanish]>" targets:<player>

        # UPDATE ACTIONBAR EVERY SECOND
        on delta time secondly every:2:

            # STOP IF ENABLE SET TO FALSE IN DATA
            - stop if:<script[ActionBar_Data].parsed_key[enable].equals[true].not>

            - define vanished false

            - foreach <server.online_players> as:__player:

                - define vanished <placeholder[cmi_user_vanished_symbol].equals[].not> if:<player.is_op>

                - if <player.gamemode.equals[survival]>:

                    # BEDROCK SIDE
                    - if <player.client_brand.equals[geyser]>:
                        - actionbar "<script[ActionBar_Data].parsed_key[bedrock.text]>" targets:<player>

                    # JAVA SIDE
                    - else:

                        # DETACT PLAYER IF PLAYER HAS RESOURCEPACK
                        - if <player.has_flag[accept_resourcepack]>:

                            # IF PLAYER NEED TO UP THE MANA BAR
                            - if <player.oxygen.is_less_than[15]>:

                                # IF PLAYER VANISH
                                - if <[vanished]>:
                                    - actionbar "<script[ActionBar_Data].parsed_key[java.survival.up.vanish]>" targets:<player>

                                # IF PLAYER NOT VANISH
                                - else:
                                    - actionbar "<script[ActionBar_Data].parsed_key[java.survival.up.unvanish]>" targets:<player>

                            - else if <player.vehicle.health_max.is_more_than[21].if_null[]>:

                                - if <player.has_flag[submerged]>:
                                    - flag <player> submerged:!
                                # IF PLAYER VANISH
                                - if <[vanished]>:
                                    - actionbar "<script[ActionBar_Data].parsed_key[java.survival.up.vanish]>" targets:<player>

                                # IF PLAYER NOT VANISH
                                - else:
                                    - actionbar "<script[ActionBar_Data].parsed_key[java.survival.up.unvanish]>" targets:<player>

                            # IF PLAYER NO NEED TO UP THE MANA BAR
                            - else:

                                # IF PLAYER VANISH
                                - if <[vanished]>:
                                    - actionbar "<script[ActionBar_Data].parsed_key[java.survival.down.vanish]>" targets:<player>

                                # IF PLAYER NOT VANISH
                                - else:
                                    - actionbar "<script[ActionBar_Data].parsed_key[java.survival.down.unvanish]>" targets:<player>

                # DETECT IF PLAYER GAMEMODE IS NOT SURVIVAL
                - else:
                    # IF PLAYER VANISH
                    - if <[vanished]>:
                        - actionbar "<script[ActionBar_Data].parsed_key[java.creative.vanish]>" targets:<player>

                    - else:
                        - foreach next

